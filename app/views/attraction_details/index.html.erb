<p id="notice"><%= notice %></p>

<h1>Listing Tourist Attraction Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attraction_details.each do |attraction_detail| %>
      <tr>
        <td><%= link_to 'Show', attraction_detail %></td>
        <td><%= link_to 'Edit', edit_attraction_detail_path(attraction_detail) %></td>
        <td><%= link_to 'Destroy', attraction_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tourist attraction detail', new_attraction_detail_path %>
